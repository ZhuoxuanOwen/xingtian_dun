<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"  " http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.custom.i18n.resources" value="jq_message"></constant> 
	<!-- define by Gao Jiangtao this project  default pachage , and other package must extend this to achieve some service -->
	<package name="zhuoxuan-struts-default" extends="struts-default">

			<interceptors>  
				<!-- 定义拦截器栈 -->
				<interceptor-stack name="starsoft-default-stack">
					<!-- <interceptor-ref name="sessionInValideInterceptor"></interceptor-ref>
					<interceptor-ref name="logInterceptor"></interceptor-ref> -->
					<interceptor-ref name="defaultStack" />
				</interceptor-stack>
			</interceptors>
			
			<!-- 修改默认的拦截器栈 -->
			<default-interceptor-ref name="starsoft-default-stack"></default-interceptor-ref>

			<!-- 全局的Session失效页面 -->	
			<global-results >
				<result name="sessionInValide">/jqdev/error_message.jsp</result>
			</global-results>

	</package>
	
	
	<package name="zhuoxuan-json-default" extends="json-default">

			<interceptors>
				<!--定义拦截器  -->
			    <!-- 定义拦截器栈 -->
				<interceptor-stack name="starsoft-default-stack">
					<!-- <interceptor-ref name="sessionInValideInterceptor"></interceptor-ref>
					<interceptor-ref name="logInterceptor"></interceptor-ref> -->
					<interceptor-ref name="defaultStack" />
				</interceptor-stack>
			</interceptors>
			
			<!-- 修改默认的拦截器栈 -->
			<default-interceptor-ref name="starsoft-default-stack"></default-interceptor-ref>

			<!-- 全局的Session失效页面 -->	
			<global-results >
				<result name="sessionInValide">/jqdev/error_message.jsp</result>
			</global-results>

	</package>
	
 
 
 
	<include file="struts-resource.xml"></include> 
</struts>